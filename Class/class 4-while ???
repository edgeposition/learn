#include <stdio.h>
#include <math.h>
int main()
{
    int a=0,b=0,c=0,d=0;
    char s='0';

       这→ while (s != '\0') 这
        {   
            scanf("%c", &s);
            if ( (s>=65 && s<=90) || (s>=97 && s<=122) ) { a++; }
            else if (s == 32) { b++; }
            else if (s >= 48 && s <= 57) { c++; }
            else if ((33 <= s && s <= 47) || (58 <= s && s <= 64) || (91 <= s && s <= 96) || (123 <= s && s <= 126))
            {d++;}
            else 这
                break;
        }

    printf("%d %d %d %d",a,b,c,d);
    return 0;

}
